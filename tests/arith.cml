print("{} + {} = {}\n", 5, 2, 5 + 2); // 5 + 2 = 7
print("{} * {} = {}\n", 5, 2, 5 * 2); // 5 * 2 = 10
print("{} - {} = {}\n", 5, 2, 5 - 2); // 5 - 2 = 3
print("{} / {} = {}\n", 5, 2, 5 / 2); // 5 / 2 = 2

print("{} + {} + {} = {}\n", 1, 2, 3, 1 + 2 + 3); // 1 + 2 + 3 = 6
print("{} + {} * {} = {}\n", 2, 3, 4, 2 + 3 * 4); // 2 + 3 * 4 = 14

print("{} == {} + {} = {}\n", 2, 1, 1, 2 == 1 + 1); // 2 == 1 + 1 = true
